+
+JHtml::_('behavior.modal', 'button.modal_' . $item);
+?>
+<button class="btn-gray modal_<?php echo $item; ?>" title="<?php echo $label; ?>" href="<?php echo $link; ?>" rel="{handler: 'iframe', size: {x: 800, y: 500}}">
+	<span class="icon-archive"></span><?php echo $label; ?>
+</button>
diff --git a/html/layouts/joomla/form/field/file.php b/html/layouts/joomla/form/field/file.php
