+        display: none;
+    }
+    &:after {
+        content: '';
+        position: absolute;
+        transition: transform 0.3s cubic-bezier(.13,.5,.66,.94);
+        width: 100%;
+        height: 100%;
+        bottom: 0;
+        top: 0;
